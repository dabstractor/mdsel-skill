{
  "name": "mdsel-claude",
  "version": "1.0.0",
  "description": "Claude Code adapter with behavioral enforcement for selector-based Markdown access",
  "homepage": "https://github.com/dabstractor/mdsel-claude",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dabstractor/mdsel-claude.git"
  },
  "bugs": {
    "url": "https://github.com/dabstractor/mdsel-claude/issues"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "mdsel",
    "markdown",
    "selector",
    "claude",
    "claude-code",
    "cli"
  ],
  "author": "Dustin",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mdsel-claude": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "hooks"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "tsup --watch",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "mdsel": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "mdsel": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "mcpName": "io.github.dabstractor/mdsel-claude"
}
